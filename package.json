{
  "name": "ui-challenges-vanilla",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "npx prettier . --write",
    "format:check": "npx prettier . --check",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "app:gen": "tsx --env-file=.env scripts/generate-new-app.ts",
    "postapp:gen": "npm run app:update",
    "app:update": "tsx --env-file=.env scripts/update-apps.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.8",
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.17.19",
    "@vitest/coverage-v8": "^3.0.6",
    "daisyui": "^5.0.0-beta.8",
    "ejs": "^3.1.10",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "jsdon": "^0.2.3",
    "prettier": "^3.5.2",
    "tailwindcss": "^4.0.8",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^5.4.14",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  }
}
